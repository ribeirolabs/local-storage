{
  "name": "@ribeirolabs/local-storage",
  "version": "0.1.0",
  "description": "Local storage helper for set/get values, with value conversion and expiration",
  "main": "dist/index.js",
  "scripts": {
    "commit": "cz",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ribeirolabs/local-storage.git"
  },
  "keywords": [
    "local",
    "storage",
    "helper",
    "expiration"
  ],
  "author": "Igor Ribeiro <igor.ribeiro.plus@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ribeirolabs/local-storage/issues"
  },
  "homepage": "https://github.com/ribeirolabs/local-storage#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.2",
    "@types/jest": "27.4.0",
    "@types/jsdom": "16.2.14",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "0.14.21",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "semantic-release-cli": "5.4.4",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && node_modules/.bin/cz --hook || true"
    }
  }
}
